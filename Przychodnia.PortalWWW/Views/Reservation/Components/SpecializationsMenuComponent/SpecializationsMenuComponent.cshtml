@model IEnumerable<Przychodnia.Database.Data.Visits.Specialization>

<div class="nav-wrapper container">
    <a id="logo-container" href="@Url.Action("MainPage","Home")" class="brand-logo">MEDICA</a>
    <ul class="right hide-on-med-and-down">
        @foreach (var item in Model)
        {
            <li>
                @Html.ActionLink(item.Name,"Index","Reservation", new {id=item.Id}, null)
            </li>
        }
        <li>
            @Html.ActionLink("Koszyk","Index","Basket")
        </li>
    </ul>

    <ul id="nav-mobile" class="sidenav">
        @foreach (var item in Model)
        {
            <li>
                @Html.ActionLink(item.Name,"Index","Reservation", new {id=item.Id}, null)
            </li>
        }
        <li>
            @Html.ActionLink("Koszyk","Index","Basket")
        </li>
    </ul>
    <a href="#" data-target="nav-mobile" class="sidenav-trigger"><i class="material-icons">menu</i></a>
</div>